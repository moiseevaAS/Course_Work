<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="453.0" prefWidth="446.0" stylesheets="@App.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.ClientController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="1.0" layoutY="-90.0" prefHeight="473.0" prefWidth="445.0" spacing="15.0">
            <children>
                <Label text="Клиент" textFill="#5a9e8e" />
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <Label fx:id="labelId" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="93.0" styleClass="heading-2" text="Id" />
                        <TextField fx:id="fieldId" prefHeight="39.0" prefWidth="279.0" promptText="введите id" styleClass="field-line" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <Label fx:id="labelFirstName" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="93.0" styleClass="heading-2" text="Имя" />
                        <TextField fx:id="fieldFirstName" prefHeight="39.0" prefWidth="279.0" promptText="введите имя" styleClass="field-line" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <Label fx:id="labelLastName" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="97.0" styleClass="heading-2" text="Фамилия" />
                        <TextField fx:id="fieldLastName" prefHeight="39.0" prefWidth="282.0" promptText="введите фамилию" styleClass="field-line" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <Label fx:id="labelFatherName" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="103.0" styleClass="heading-2" text="Отчество" />
                        <TextField fx:id="fieldFatherName" prefHeight="39.0" prefWidth="287.0" promptText="введите отчество" styleClass="field-line" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <Label fx:id="labelPassportSer" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="192.0" styleClass="heading-2" text="Паспорт серия" />
                        <TextField fx:id="fieldPassportSer" prefHeight="39.0" prefWidth="271.0" promptText="введите серию" styleClass="field-line" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <Label fx:id="labelPassportNum" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="192.0" styleClass="heading-2" text="Паспорт номер" />
                        <TextField fx:id="fieldPassportNum" prefHeight="39.0" prefWidth="271.0" promptText="введите номер" styleClass="field-line" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="25.0">
                    <children>
                        <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#onClickAdd" prefHeight="39.0" prefWidth="110.0" styleClass="btn-info" text="Подтвердить" />
                        <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#onClickCancel" styleClass="btn-info" text="Назад" />
                    </children>
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </HBox>
            </children>
            <padding>
                <Insets bottom="25.0" left="25.0" right="25.0" top="125.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
